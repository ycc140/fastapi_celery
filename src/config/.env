# Service information
VERSION="1.3.0"
SERVICE_NAME='ProcessingService'
NAME="FastAPI-Celery-Flower example API"

# Environment parameters
FLOWER_HOST="localhost"

# Logging parameters.
LOG_LEVEL="debug"
LOG_DIAGNOSE=true
LOG_FORMAT="<level>{time:YYYY-MM-DD HH:mm:ss.SSS} | {level:<8} | {module}:{function}:{line} - {message}</level>"

# Authentication user.
SERVICE_USER="service_user"
